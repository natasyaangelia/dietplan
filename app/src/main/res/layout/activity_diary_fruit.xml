<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@android:color/background_light"
    android:paddingTop="@dimen/activity_vertical_margin"
    tools:context=".DiaryFruitActivity">

    <EditText
        android:layout_width="350dp"
        android:layout_height="35dp"
        android:inputType="textEmailAddress"
        android:background="@drawable/kolomsearch"
        android:drawableLeft="@drawable/searchicon"
        android:ems="10"
        android:textSize="@dimen/activity_horizontal_margin"
        android:id="@+id/searchEnergyDiary"
        android:hint="Search"
        android:paddingLeft="15dp"
        android:layout_marginRight="18dp"
        android:layout_marginLeft="18dp"
        android:layout_alignParentTop="true" />

    <Button
        android:layout_width="60dp"
        android:layout_height="60dp"
        android:background="@drawable/fruit"
        android:id="@+id/btnFruit"
        android:onClick="gotoFruit"
        android:layout_below="@+id/searchEnergyDiary"
        android:layout_marginTop="15dp"
        android:layout_marginLeft="83dp"/>

    <Button
        android:layout_width="60dp"
        android:layout_height="60dp"
        android:background="@drawable/vegetable"
        android:id="@+id/btnVegetable"
        android:onClick="gotoVegetable"
        android:layout_alignTop="@+id/btnProtein"
        android:layout_centerHorizontal="true" />

    <Button
        android:layout_width="60dp"
        android:layout_height="60dp"
        android:background="@drawable/protein"
        android:id="@+id/btnProtein"
        android:onClick="gotoProtein"
        android:layout_marginLeft="217dp"
        android:layout_alignTop="@+id/btnFruit" />

    <Button
        android:layout_width="60dp"
        android:layout_height="60dp"
        android:background="@drawable/drink"
        android:id="@+id/btnDrink"
        android:onClick="gotoDrink"
        android:layout_below="@+id/btnFruit"
        android:layout_marginTop="5dp"
        android:layout_marginLeft="83dp"/>

    <Button
        android:layout_width="60dp"
        android:layout_height="60dp"
        android:background="@drawable/othersnack"
        android:id="@+id/btnOtherSnack"
        android:onClick="gotoOtherSnack"
        android:layout_below="@+id/btnFruit"
        android:layout_marginTop="5dp"
        android:layout_centerHorizontal="true"
        android:layout_marginLeft="100dp"/>

    <Button
        android:layout_width="60dp"
        android:layout_height="60dp"
        android:background="@drawable/oat8products"
        android:id="@+id/btnOat8"
        android:onClick="gotoOat8"
        android:layout_above="@+id/imageView3"
        android:layout_alignLeft="@+id/btnProtein"
        android:layout_alignStart="@+id/btnProtein" />

    <ImageView
        android:layout_width="match_parent"
        android:layout_height="30dp"
        android:id="@+id/imageView3"
        android:layout_marginTop="15dp"
        android:layout_below="@id/btnDrink"
        android:layout_centerVertical="true"
        android:background="@drawable/bgbmi"/>

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Fruit"
        android:layout_marginTop="15dp"
        android:textColor="@color/brownText"
        android:layout_below="@id/btnDrink"
        android:id="@+id/txtJudul"
        android:layout_centerVertical="true"
        android:layout_centerHorizontal="true" />

    <TextView
        android:layout_width="150dp"
        android:layout_height="180dp"
        android:layout_marginTop="20dp"
        android:background="@drawable/listfruit"
        android:layout_below="@+id/imageView3"
        android:layout_alignLeft="@+id/searchEnergyDiary"
        android:layout_alignStart="@+id/searchEnergyDiary"
        android:id="@+id/textView" />

    <TextView
        android:layout_width="45dp"
        android:layout_height="180dp"
        android:background="@drawable/listfruittotal"
        android:layout_alignTop="@+id/textView"
        android:layout_toRightOf="@+id/btnOtherSnack"
        android:layout_toEndOf="@+id/btnOtherSnack"
        android:id="@+id/textView2" />

    <TextView
        android:layout_width="45dp"
        android:layout_height="180dp"
        android:background="@drawable/kallist"
        android:id="@+id/textView3"
        android:layout_alignTop="@+id/apelKal"
        android:layout_alignRight="@+id/searchEnergyDiary"
        android:layout_alignEnd="@+id/searchEnergyDiary" />

    <ImageView
        android:layout_width="match_parent"
        android:layout_height="30dp"
        android:id="@+id/imageView5"
        android:layout_marginTop="5dp"
        android:background="@drawable/bgbmi"
        android:layout_below="@+id/textView"/>

    <Button
        android:layout_width="100dp"
        android:layout_height="40dp"
        android:background="@drawable/buttonlink"
        android:textSize="10dp"
        android:onClick="addHistory"
        android:textColor="@android:color/background_light"
        android:text="Add to diary"
        android:id="@+id/button"
        android:layout_centerHorizontal="true"
        android:layout_alignBottom="@+id/imageView5"
        android:layout_alignTop="@+id/imageView5" />

    <TextView
        android:gravity="center"
        android:layout_width="40dp"
        android:layout_height="wrap_content"
        android:text="x"
        android:id="@+id/apel"
        android:layout_alignTop="@+id/textView2"
        android:layout_alignLeft="@+id/mangga"
        android:layout_alignStart="@+id/mangga" />

    <TextView
        android:gravity="center"
        android:layout_width="40dp"
        android:layout_height="wrap_content"
        android:text="x"
        android:id="@+id/mangga"
        android:layout_marginTop="17dp"
        android:layout_below="@+id/apel"
        android:layout_alignRight="@+id/textView2"
        android:layout_alignEnd="@+id/textView2" />

    <TextView
        android:gravity="center"
        android:layout_width="40dp"
        android:layout_height="wrap_content"
        android:text="x"
        android:id="@+id/pepaya"
        android:layout_marginTop="17dp"
        android:layout_below="@+id/mangga"
        android:layout_alignLeft="@+id/mangga"
        android:layout_alignStart="@+id/mangga" />

    <TextView
        android:gravity="center"
        android:layout_width="40dp"
        android:layout_height="wrap_content"
        android:text="x"
        android:id="@+id/pisang"
        android:layout_marginTop="13dp"
        android:layout_below="@+id/pepaya"
        android:layout_alignLeft="@+id/pepaya"
        android:layout_alignStart="@+id/pepaya" />

    <TextView
        android:gravity="center"
        android:layout_width="40dp"
        android:layout_height="wrap_content"
        android:text="x"
        android:id="@+id/semangka"
        android:layout_alignBottom="@+id/textView2"
        android:layout_alignLeft="@+id/pisang"
        android:layout_alignStart="@+id/pisang"
        android:layout_marginBottom="19dp" />

    <TextView
        android:gravity="center"
        android:layout_width="40dp"
        android:layout_height="wrap_content"
        android:text="x"
        android:id="@+id/apelKal"
        android:layout_alignTop="@+id/apel"
        android:layout_toRightOf="@+id/manggaMinus"
        android:layout_toEndOf="@+id/manggaMinus" />

    <TextView
        android:gravity="center"
        android:layout_width="40dp"
        android:layout_height="wrap_content"
        android:text="x"
        android:id="@+id/manggaKal"
        android:layout_above="@+id/pepaya"
        android:layout_alignLeft="@+id/apelKal"
        android:layout_alignStart="@+id/apelKal" />

    <TextView
        android:gravity="center"
        android:layout_width="40dp"
        android:layout_height="wrap_content"
        android:text="x"
        android:id="@+id/pepayaKal"
        android:layout_alignTop="@+id/pepaya"
        android:layout_alignLeft="@+id/manggaKal"
        android:layout_alignStart="@+id/manggaKal" />

    <TextView
        android:gravity="center"
        android:layout_width="40dp"
        android:layout_height="wrap_content"
        android:text="x"
        android:id="@+id/pisangKal"
        android:layout_alignTop="@+id/pisang"
        android:layout_alignLeft="@+id/pepayaKal"
        android:layout_alignStart="@+id/pepayaKal" />

    <TextView
        android:gravity="center"
        android:layout_width="40dp"
        android:layout_height="wrap_content"
        android:text="x"
        android:id="@+id/semangkaKal"
        android:layout_alignTop="@+id/semangka"
        android:layout_alignLeft="@+id/pisangKal"
        android:layout_alignStart="@+id/pisangKal" />

    <!--Button-->
    <!--Plus-->
    <Button
        android:layout_width="15dp"
        android:layout_height="15dp"
        android:background="@drawable/plus"
        android:id="@+id/apelPlus"
        android:onClick="plusapel"
        android:layout_above="@+id/mangga"
        android:layout_alignLeft="@+id/manggaPlus"
        android:layout_alignStart="@+id/manggaPlus" />

    <Button
        android:layout_width="15dp"
        android:layout_height="15dp"
        android:background="@drawable/plus"
        android:id="@+id/manggaPlus"
        android:onClick="plusmangga"
        android:layout_above="@+id/pepaya"
        android:layout_alignRight="@+id/txtJudul"
        android:layout_alignEnd="@+id/txtJudul" />

    <Button
        android:layout_width="15dp"
        android:layout_height="15dp"
        android:background="@drawable/plus"
        android:id="@+id/pepayaPlus"
        android:onClick="pluspepaya"
        android:layout_above="@+id/pisang"
        android:layout_alignLeft="@+id/apelPlus"
        android:layout_alignStart="@+id/apelPlus" />

    <Button
        android:layout_width="15dp"
        android:layout_height="15dp"
        android:background="@drawable/plus"
        android:id="@+id/pisangPlus"
        android:onClick="pluspisang"
        android:layout_alignBottom="@+id/pisang"
        android:layout_alignLeft="@+id/pepayaPlus"
        android:layout_alignStart="@+id/pepayaPlus" />

    <Button
        android:layout_width="15dp"
        android:layout_height="15dp"
        android:background="@drawable/plus"
        android:id="@+id/semangkaPlus"
        android:onClick="plussemangka"
        android:layout_alignBottom="@+id/semangka"
        android:layout_alignLeft="@+id/pisangPlus"
        android:layout_alignStart="@+id/pisangPlus" />

    <!--Minus-->

    <Button
        android:layout_width="15dp"
        android:layout_height="15dp"
        android:background="@drawable/minus"
        android:id="@+id/apelMinus"
        android:onClick="minmangga"
        android:layout_above="@+id/pepaya"
        android:layout_alignLeft="@+id/manggaMinus"
        android:layout_alignStart="@+id/manggaMinus" />

    <Button
        android:layout_width="15dp"
        android:layout_height="15dp"
        android:background="@drawable/minus"
        android:id="@+id/manggaMinus"
        android:onClick="minapel"
        android:layout_above="@+id/mangga"
        android:layout_toLeftOf="@+id/textView3"
        android:layout_toStartOf="@+id/textView3"
        android:layout_marginRight="10dp"/>

    <Button
        android:layout_width="15dp"
        android:layout_height="15dp"
        android:background="@drawable/minus"
        android:id="@+id/pepayaMinus"
        android:onClick="minpepaya"
        android:layout_alignBottom="@+id/pepayaKal"
        android:layout_alignLeft="@+id/apelMinus"
        android:layout_alignStart="@+id/apelMinus" />

    <Button
        android:layout_width="15dp"
        android:layout_height="15dp"
        android:background="@drawable/minus"
        android:id="@+id/pisangMinus"
        android:onClick="minpisang"
        android:layout_alignBottom="@+id/pisangKal"
        android:layout_alignLeft="@+id/pepayaMinus"
        android:layout_alignStart="@+id/pepayaMinus" />

    <Button
        android:layout_width="15dp"
        android:layout_height="15dp"
        android:background="@drawable/minus"
        android:id="@+id/semangkaMinus"
        android:onClick="minsemangka"
        android:layout_alignBottom="@+id/semangkaKal"
        android:layout_alignLeft="@+id/pisangMinus"
        android:layout_alignStart="@+id/pisangMinus" />

</RelativeLayout>
